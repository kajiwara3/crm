<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
    </tr>
  </thead>
  <tbody>
    <% @target_list.each do |target| %>
      <tr>
        <td><%= target.name %></td>
        <td><%= target.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @target_list %>
